<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wiwj.screen.mapper.UserMapper">
    <select id="userLogin" parameterType="com.wiwj.screen.model.User" resultType="com.wiwj.screen.model.User">
        select * from user where userid = #{userid} and pass = #{pass}
    </select>
</mapper>